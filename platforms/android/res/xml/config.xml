<?xml version='1.0' encoding='utf-8'?>
<widget id="com.danielleklaasen.ar" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="WikitudePlugin">
        <param name="android-package" value="com.wikitude.phonegap.WikitudePlugin" />
    </feature>
    <name>Amager AR</name>
    <description>
        See the renovation of amagerbrogade through augmented reality.
    </description>
    <author email="danielleklaasen@gmail.com" href="http://www.danielleklaasen.com">
        Danielle Klaasen
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/icon.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
